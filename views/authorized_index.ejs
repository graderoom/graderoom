                    $("#assignmentDate" + i + j).text(assignmentDates[assignmentIndex]);

                    // Set up names
                    $("#assignmentName" + i + j).text(assignmentNames[assignmentIndex]);

                    // Set up categories
                    let dropdown = $("#assignmentCategory" + i + j).children("select");
                    let weightNames = Object.keys(weights[Object.keys(weights)[i]]["weights"]);

                    // Bind listeners only once
                    if (!dropdown.find("option").length) {
