<!-- Session timeout dialog -->
<div class="blurred-login session-timeout">
    <div class="blurred-background session-timeout"></div>
    <div class="col-sm-11 col-md-10 col-lg-9 col-xl-8 mx-auto session-timeout">
        <div id="privacyCard" class="card card-signin my-5 session-timeout">
            <div class="card-body session-timeout">
                <h5 class="card-title text-center session-timeout">You've been logged out</h5>
                <div class="text-center session-timeout">
                    <button class="btn btn-sm session-timeout" onclick="window.location.reload()">Return to Login
                    </button>
                </div>
            </div>
        </div>
    </div>
</div>
<script>
    // Show user when session ends
    let timeout = <%= sessionTimeout %>;
    setInterval(function () {

        if (Date.now() > timeout) {
            $(".session-timeout").show();
            $("body").find("*").not(".session-timeout").remove();
        }

    }, 10000);
</script>
